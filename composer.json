{
    "name": "owenvoke/skeleton-php",
    "type": "library",
    "description": ":package_description",
    "license": "MIT",
    "require": {
        "php": "^7.4"
    },
    "require-dev": {
        "pestphp/pest": "^0.1.5",
        "phpstan/phpstan": "^0.12.5"
    },
    "autoload": {
        "psr-4": {
            "OwenVoke\\Skeleton\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "OwenVoke\\Skeleton\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "pest",
        "analyse": "phpstan analyse"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
